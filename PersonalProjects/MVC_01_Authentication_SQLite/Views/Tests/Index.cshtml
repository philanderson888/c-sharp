@model IEnumerable<MVC_01_Authentication_SQLite.Data.Test>

@{
    ViewData["Title"] = "Index";
}

<h1>Index From Phil To List Test Items</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.testName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.testName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.testid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.testid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.testid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
